        public int countGenes(String dna)
    {
        int startIndex = 0 ;
        int numberOfGenes=0;
        while (true)
        {
            String gene = findGene(dna,startIndex);
            if(gene.isEmpty())
            {
                break;
            }
            numberOfGenes++;
            startIndex=(dna.toUpperCase().indexOf((gene.toUpperCase()),startIndex))+gene.length();
        }
        return numberOfGenes;
    }
    public void testCountGenes()
    {
         FileResource fr = new FileResource("brca1line.fa");
         String dna = fr.asString();
         
         System.out.println("in this files there is " + countGenes(dna) + "genes");
         
    }
    
       public int CTG_codon ( String dna)
    {
        int num=0;
        String temp= dna.toUpperCase();
        for( int i=0; i<dna.length(); i++)
        {
            if(temp.charAt(i)== 'C' && temp.charAt(i+1)== 'T' && temp.charAt(i+2) == 'G')
            {
             num++;   
            }
            
        }
        return  num;
    }
    
    public void printCTG_codn ()
    {
        String dna = "" ;
        System.out.println("numbers of occurences of CTG codon in this DNA is : " + CTG_codon(dna));
    }
        
